extends layout

block content
  div.container
    .row
      a(href='/logout') Logout
      h1 Hello #{user.email}
      div.content.col-md-4.col-md-offset-4
        h1 Curs Valutar BNR
        table.table.table-striped.table-hover
          thead
            tr
              th Nume
              th Valoare
              th Data
          tbody
            tr
              td.name1
              td.val1
              td.date
             tr
              td.name2
              td.val2
              td.date
             tr
              td.name3
              td.val3
              td.date
        h1 Evolutie Curs
        div.nav-collapse
          .dropdown
            a(data-toggle='dropdown', href='#menu1').btn.btn-default.dropdown-toogle Change View 
              b.caret
            ul.dropdown-menu
        div.ct-chart.ct-golden-section
        footer
          p by Alex